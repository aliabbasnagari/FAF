{
    "manifest_version": 3,
    "name": "FAF",
    "version": "1.0",
    "description": "FAF (Flex Auto Feedback) a tool that Auto submits Course feedbacks based on single preference choice. (By Ali Abbas)ðŸ˜œ",
    "permissions": [
        "activeTab",
        "scripting",
        "webNavigation"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "256": "images/icon.png"
        }
    },
    "icons": {
        "256": "images/icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://flexstudent.nu.edu.pk/Student/FeedBackQuestions/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "background": {
        "service_worker": "background.js"
    }
}